<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      login-required="true"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">

	<rewrite pattern="/instructor/incomeList"/>
	<rewrite pattern="/instructor/incomeList/{courseId}"/>

	<param name="firstResult" value="#{incomeQuery.firstResult}" />
	<param name="pageNumber" value="#{incomeQuery.pageNumber}" />


	<!-- Sorting -->
	<param name="sort" value="#{incomeQuery.orderColumn}"/>
