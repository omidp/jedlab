<div class="card" id="block_{{id}}">
				    <div class="w3-teal">
				      <div class="left floated meta">
								<div id="delblock_{{id}}"  onclick="curate.deletePageBlock({{id}});" 
										class="right floated mini ui button w3-margin"><i class="icon-bin" />
								</div>				      	
				      </div>
				      <label class="pull-left w3-xxlarge w3-margin-right" data-block-id="{{id}}" data-block-title="{{title}}">{{title}}</label>
					  <input class="clickedit {{id}}" type="text" value="{{title}}"/>					  
				    </div> 
				    <div class="content">
						<div class="ui ordered divided list" style="direction:ltr;" id="ul_{{id}}">
						
						</div>
				    </div>
				    <div class="extra content">
				    	<div class="ui action input">
						  <input placeholder="URL" type="text" id="curinp_{{id}}" style="direction: ltr;width:80%;text-align: left;"/>
						  <button class="ui icon button addcur" id="curbtn_{{id}}" data-block-id="{{id}}" onclick="curate.addCurate({{id}})"> 
						    <i class="icon-plus"></i>
						  </button>
						</div>
				    </div>
</div>