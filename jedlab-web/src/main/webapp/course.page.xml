<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd">

	
	 <rewrite pattern="/course/{courseId}" />
	<param name="courseId" value="#{courseHome.courseId}" required="true"/>
	<param name="courseId" value="#{chapterQuery.course.id}" />
	
	
	<navigation from-action="#{courseHome.register()}">
		<end-conversation />
		<rule if-outcome="registered">
			<redirect view-id="/member/course.xhtml" include-page-params="false">
				<param name="courseId" value="#{courseId}"/>
			</redirect>
		</rule>
		<rule if-outcome="gotoBank">
			
		</rule>
		<rule if-outcome="notRegistered">
			<redirect view-id="/member/course.xhtml" include-page-params="false">
				<param name="courseId" value="#{courseId}"/>
				<message>#{messages['Unable_To_Register']}</message>
			</redirect>
		</rule>
	</navigation>
	

</page>
